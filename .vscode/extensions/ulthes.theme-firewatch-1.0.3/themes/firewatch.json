      "scope": "markup.bold",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#C8AE9D"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#E06C75"
      }
    }
